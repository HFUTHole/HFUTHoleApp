{
  "expo": {
    "name": "工大树洞",
    "slug": "hfuthole",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.jpg",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#2F6F31"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hfuthole.app",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.jpg"
      },
      "permissions": [
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ],
      "package": "com.hfuthole.app"
    },
    "web": {
      "favicon": "./assets/splash.jpg"
    },
    "extra": {
      "eas": {
        "projectId": "dba5ed50-696a-4891-ab2e-cd94660925e7"
      }
    },
    "owner": "snowingfox"
  }
}
